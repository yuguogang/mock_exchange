{
  "api_endpoint_open_orders": "/bapi/futures/v1/private/future/order/open-orders",
  "api_endpoint_order_history": "/bapi/futures/v1/private/future/order/order-history",
  "method": "POST",
  "description": "获取用户订单信息（当前挂单和历史订单）",
  "open_orders_query_parameters": {},
  "open_orders_postData_parameters": {},
  "order_history_postData_parameters": {
    "startTime": {
      "type": "number",
      "description": "开始时间戳（毫秒）",
      "example": 1767628800000,
      "required": false
    },
    "endTime": {
      "type": "number",
      "description": "结束时间戳（毫秒）",
      "example": 1769183999999,
      "required": false
    },
    "accountType": {
      "type": "string",
      "description": "账户类型",
      "example": "MAIN",
      "required": false,
      "default": "MAIN"
    },
    "statusList": {
      "type": "array",
      "description": "订单状态列表",
      "example": ["CANCELED", "EXPIRED", "FILLED", "PARTIALLY_FILLED", "EXPIRED_IN_MATCH"],
      "enum": ["NEW", "PARTIALLY_FILLED", "FILLED", "CANCELED", "EXPIRED", "EXPIRED_IN_MATCH"],
      "required": false
    },
    "rows": {
      "type": "number",
      "description": "每页记录数",
      "example": 100,
      "required": false,
      "default": 100
    },
    "page": {
      "type": "number",
      "description": "页码",
      "example": 1,
      "required": false,
      "default": 1
    }
  },
  "response_fields": {
    "orderId": "订单ID",
    "symbol": "交易对符号",
    "clientOrderId": "客户端订单ID",
    "price": "订单价格",
    "origQty": "原始数量",
    "executedQty": "已成交数量",
    "executedQuoteQty": "已成交报价数量",
    "status": "订单状态",
    "timeInForce": "有效时间",
    "type": "订单类型",
    "side": "订单方向",
    "stopPrice": "止损价格",
    "insertTime": "插入时间",
    "updateTime": "更新时间",
    "avgPrice": "平均成交价格",
    "positionSide": "持仓方向"
  },
  "order_status": ["NEW", "PARTIALLY_FILLED", "FILLED", "CANCELED", "EXPIRED", "EXPIRED_IN_MATCH"],
  "order_types": ["LIMIT", "MARKET", "STOP", "STOP_MARKET", "TAKE_PROFIT", "TAKE_PROFIT_MARKET"],
  "time_in_force": ["GTC", "IOC", "FOK"]
}
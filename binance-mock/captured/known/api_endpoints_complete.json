{
  "api_documentation": {
    "version": "1.0",
    "description": "币安合约交易关键接口参数整理",
    "last_updated": "2026-01-26"
  },
  
  "endpoints": {
    "trade_history": {
      "endpoint": "/bapi/futures/v1/private/future/user-data/trade-history",
      "method": "POST",
      "description": "获取用户交易历史记录",
      "postData": {
        "startTime": {
          "type": "number",
          "description": "开始时间戳（毫秒）",
          "example": 1767628800000,
          "required": false,
          "format": "Unix timestamp in milliseconds"
        },
        "endTime": {
          "type": "number", 
          "description": "结束时间戳（毫秒）",
          "example": 1769183999998,
          "required": false,
          "format": "Unix timestamp in milliseconds"
        },
        "page": {
          "type": "number",
          "description": "页码，从1开始",
          "example": 1,
          "required": false,
          "default": 1,
          "minimum": 1
        },
        "rows": {
          "type": "number",
          "description": "每页记录数",
          "example": 200,
          "required": false,
          "default": 200,
          "maximum": 500
        },
        "symbol": {
          "type": "string",
          "description": "交易对符号，如TRXUSDT",
          "example": "TRXUSDT",
          "required": false,
          "pattern": "^[A-Z]+USDT$"
        },
        "side": {
          "type": "string",
          "description": "交易方向过滤",
          "example": "SELL",
          "enum": ["BUY", "SELL"],
          "required": false
        }
      },
      "response_fields": {
        "tradeId": {"type": "number", "description": "交易ID"},
        "orderId": {"type": "number", "description": "订单ID"},
        "orderIdStr": {"type": "string", "description": "订单ID字符串"},
        "price": {"type": "string", "description": "成交价格"},
        "insertTime": {"type": "number", "description": "插入时间戳"},
        "symbol": {"type": "string", "description": "交易对"},
        "side": {"type": "string", "description": "交易方向"},
        "activeBuy": {"type": "boolean", "description": "是否主动买入"},
        "qty": {"type": "string", "description": "成交数量"},
        "fee": {"type": "string", "description": "手续费"},
        "commissionAsset": {"type": "string", "description": "手续费资产"},
        "totalQuota": {"type": "string", "description": "总成交额"},
        "realizedProfit": {"type": "string", "description": "已实现盈亏"},
        "positionSide": {"type": "string", "description": "持仓方向"}
      }
    },

    "transaction_history": {
      "endpoint": "/bapi/futures/v1/private/future/user-data/transaction-history",
      "method": "POST",
      "description": "获取用户资金流水记录",
      "postData": {
        "startTime": {
          "type": "number",
          "description": "开始时间戳（毫秒）",
          "example": 1767196800000,
          "required": false
        },
        "endTime": {
          "type": "number",
          "description": "结束时间戳（毫秒）", 
          "example": 1769183999998,
          "required": false
        },
        "page": {
          "type": "number",
          "description": "页码",
          "example": 1,
          "required": false,
          "default": 1
        },
        "rows": {
          "type": "number",
          "description": "每页记录数",
          "example": 200,
          "required": false,
          "default": 200
        }
      },
      "response_fields": {
        "id": {"type": "string", "description": "流水ID"},
        "tranId": {"type": "number", "description": "交易ID"},
        "asset": {"type": "string", "description": "资产类型"},
        "balancetype": {"type": "string", "description": "流水类型"},
        "balanceTypeStr": {"type": "string", "description": "流水类型字符串"},
        "balanceDelta": {"type": "number", "description": "余额变动"},
        "balanceDeltaStr": {"type": "string", "description": "余额变动字符串"},
        "balanceInfo": {"type": "string", "description": "余额信息"},
        "time": {"type": "number", "description": "时间戳"},
        "symbol": {"type": "string", "description": "交易对"}
      },
      "balance_types": [
        "COMMISSION",
        "REALIZED_PNL", 
        "FUNDING_FEE",
        "TRANSFER_IN",
        "TRANSFER_OUT",
        "MARGIN_TRANSFER",
        "SETTLE_PROFIT"
      ]
    },

    "user_position": {
      "endpoint": "/bapi/futures/v6/private/future/user-data/user-position",
      "method": "POST",
      "description": "获取用户持仓信息",
      "postData": {},
      "response_fields": {
        "id": {"type": "string", "description": "持仓ID"},
        "symbol": {"type": "string", "description": "交易对符号"},
        "positionSide": {"type": "string", "description": "持仓方向"},
        "positionAmount": {"type": "string", "description": "持仓数量"},
        "entryPrice": {"type": "string", "description": "开仓价格"},
        "breakEvenPrice": {"type": "string", "description": "盈亏平衡价格"},
        "markPrice": {"type": "string", "description": "标记价格"},
        "unrealizedProfit": {"type": "string", "description": "未实现盈亏"},
        "liquidationPrice": {"type": "string", "description": "强平价格"},
        "isolatedMargin": {"type": "string", "description": "逐仓保证金"},
        "notionalValue": {"type": "string", "description": "名义价值"},
        "collateral": {"type": "string", "description": "抵押品"},
        "isolatedWallet": {"type": "string", "description": "逐仓钱包"},
        "cumRealized": {"type": "string", "description": "累计已实现盈亏"},
        "initialMargin": {"type": "string", "description": "初始保证金"},
        "maintMargin": {"type": "string", "description": "维持保证金"},
        "positionInitialMargin": {"type": "string", "description": "持仓初始保证金"},
        "openOrderInitialMargin": {"type": "string", "description": "挂单初始保证金"},
        "adl": {"type": "number", "description": "自动减仓等级"},
        "askNotional": {"type": "string", "description": "卖单名义价值"},
        "bidNotional": {"type": "string", "description": "买单名义价值"},
        "updateTime": {"type": "number", "description": "更新时间"}
      },
      "position_sides": ["LONG", "SHORT"]
    },

    "open_orders": {
      "endpoint": "/bapi/futures/v1/private/future/order/open-orders",
      "method": "POST",
      "description": "获取用户当前挂单",
      "postData": {},
      "response_fields": {
        "orderId": {"type": "number", "description": "订单ID"},
        "symbol": {"type": "string", "description": "交易对符号"},
        "clientOrderId": {"type": "string", "description": "客户端订单ID"},
        "price": {"type": "string", "description": "订单价格"},
        "origQty": {"type": "string", "description": "原始数量"},
        "executedQty": {"type": "string", "description": "已成交数量"},
        "executedQuoteQty": {"type": "string", "description": "已成交报价数量"},
        "status": {"type": "string", "description": "订单状态"},
        "timeInForce": {"type": "string", "description": "有效时间"},
        "type": {"type": "string", "description": "订单类型"},
        "side": {"type": "string", "description": "订单方向"},
        "stopPrice": {"type": "string", "description": "止损价格"},
        "insertTime": {"type": "number", "description": "插入时间"},
        "updateTime": {"type": "number", "description": "更新时间"},
        "positionSide": {"type": "string", "description": "持仓方向"}
      }
    },

    "order_history": {
      "endpoint": "/bapi/futures/v1/private/future/order/order-history",
      "method": "POST",
      "description": "获取用户历史订单",
      "postData": {
        "startTime": {
          "type": "number",
          "description": "开始时间戳（毫秒）",
          "example": 1767628800000,
          "required": false
        },
        "endTime": {
          "type": "number",
          "description": "结束时间戳（毫秒）",
          "example": 1769183999999,
          "required": false
        },
        "accountType": {
          "type": "string",
          "description": "账户类型",
          "example": "MAIN",
          "required": false,
          "default": "MAIN"
        },
        "statusList": {
          "type": "array",
          "description": "订单状态列表",
          "example": ["CANCELED", "EXPIRED", "FILLED", "PARTIALLY_FILLED", "EXPIRED_IN_MATCH"],
          "enum": ["NEW", "PARTIALLY_FILLED", "FILLED", "CANCELED", "EXPIRED", "EXPIRED_IN_MATCH"],
          "required": false
        },
        "rows": {
          "type": "number",
          "description": "每页记录数",
          "example": 100,
          "required": false,
          "default": 100
        },
        "page": {
          "type": "number",
          "description": "页码",
          "example": 1,
          "required": false,
          "default": 1
        }
      },
      "response_fields": {
        "orderId": {"type": "number", "description": "订单ID"},
        "symbol": {"type": "string", "description": "交易对符号"},
        "clientOrderId": {"type": "string", "description": "客户端订单ID"},
        "price": {"type": "string", "description": "订单价格"},
        "origQty": {"type": "string", "description": "原始数量"},
        "executedQty": {"type": "string", "description": "已成交数量"},
        "executedQuoteQty": {"type": "string", "description": "已成交报价数量"},
        "status": {"type": "string", "description": "订单状态"},
        "timeInForce": {"type": "string", "description": "有效时间"},
        "type": {"type": "string", "description": "订单类型"},
        "side": {"type": "string", "description": "订单方向"},
        "stopPrice": {"type": "string", "description": "止损价格"},
        "insertTime": {"type": "number", "description": "插入时间"},
        "updateTime": {"type": "number", "description": "更新时间"},
        "avgPrice": {"type": "string", "description": "平均成交价格"},
        "positionSide": {"type": "string", "description": "持仓方向"}
      }
    }
  },

  "common_parameters": {
    "pagination": {
      "page": "页码，从1开始",
      "rows": "每页记录数，最大500"
    },
    "time_range": {
      "startTime": "开始时间戳（毫秒）",
      "endTime": "结束时间戳（毫秒）"
    },
    "symbol_filter": {
      "symbol": "交易对符号，如TRXUSDT"
    }
  },

  "data_types": {
    "order_status": ["NEW", "PARTIALLY_FILLED", "FILLED", "CANCELED", "EXPIRED", "EXPIRED_IN_MATCH"],
    "order_types": ["LIMIT", "MARKET", "STOP", "STOP_MARKET", "TAKE_PROFIT", "TAKE_PROFIT_MARKET"],
    "time_in_force": ["GTC", "IOC", "FOK"],
    "position_sides": ["LONG", "SHORT"],
    "trade_sides": ["BUY", "SELL"],
    "balance_types": [
      "COMMISSION",
      "REALIZED_PNL", 
      "FUNDING_FEE",
      "TRANSFER_IN",
      "TRANSFER_OUT"
    ]
  }
}
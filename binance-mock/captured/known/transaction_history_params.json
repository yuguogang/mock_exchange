{
  "api_endpoint": "/bapi/futures/v1/private/future/user-data/transaction-history",
  "method": "POST", 
  "description": "获取用户资金流水记录",
  "query_parameters": {},
  "postData_parameters": {
    "startTime": {
      "type": "number",
      "description": "开始时间戳（毫秒）",
      "example": 1767196800000,
      "required": false
    },
    "endTime": {
      "type": "number",
      "description": "结束时间戳（毫秒）", 
      "example": 1769183999998,
      "required": false
    },
    "page": {
      "type": "number",
      "description": "页码",
      "example": 1,
      "required": false,
      "default": 1
    },
    "rows": {
      "type": "number",
      "description": "每页记录数",
      "example": 200,
      "required": false,
      "default": 200
    }
  },
  "response_fields": {
    "id": "流水ID",
    "tranId": "交易ID",
    "asset": "资产类型",
    "balancetype": "流水类型",
    "balanceTypeStr": "流水类型字符串",
    "balanceDelta": "余额变动",
    "balanceDeltaStr": "余额变动字符串",
    "balanceInfo": "余额信息",
    "time": "时间戳",
    "symbol": "交易对"
  },
  "balance_types": [
    "COMMISSION",
    "REALIZED_PNL", 
    "FUNDING_FEE",
    "TRANSFER_IN",
    "TRANSFER_OUT"
  ]
}
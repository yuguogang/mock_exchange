{
  "api_endpoint": "/bapi/futures/v1/private/future/user-data/trade-history",
  "method": "POST",
  "description": "获取用户交易历史记录",
  "query_parameters": {},
  "postData_parameters": {
    "startTime": {
      "type": "number",
      "description": "开始时间戳（毫秒）",
      "example": 1767628800000,
      "required": false
    },
    "endTime": {
      "type": "number", 
      "description": "结束时间戳（毫秒）",
      "example": 1769183999998,
      "required": false
    },
    "page": {
      "type": "number",
      "description": "页码",
      "example": 1,
      "required": false,
      "default": 1
    },
    "rows": {
      "type": "number",
      "description": "每页记录数",
      "example": 200,
      "required": false,
      "default": 200
    },
    "symbol": {
      "type": "string",
      "description": "交易对符号",
      "example": "TRXUSDT",
      "required": false
    },
    "side": {
      "type": "string",
      "description": "交易方向",
      "example": "SELL",
      "enum": ["BUY", "SELL"],
      "required": false
    }
  },
  "response_fields": {
    "tradeId": "交易ID",
    "orderId": "订单ID", 
    "price": "成交价格",
    "symbol": "交易对",
    "side": "交易方向",
    "qty": "成交数量",
    "fee": "手续费",
    "commissionAsset": "手续费资产",
    "totalQuota": "总成交额",
    "realizedProfit": "已实现盈亏",
    "positionSide": "持仓方向",
    "insertTime": "插入时间"
  }
}